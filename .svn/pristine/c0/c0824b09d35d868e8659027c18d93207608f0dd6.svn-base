using ProtoBuf;
using VRage.Utils;
using Vsi.Network;

namespace Vsi.AutoMiner
{

    [ProtoContract]
    public class StopMessage : EntityMessage<StopMessage>
    {
        public static readonly int ID = (int)MyStringHash.GetOrCompute("VSI.AutominerStop");

        [ProtoMember(1)]
        public override long EntityId { get; protected set; }

        public StopMessage(long entityId)
            : base(entityId)
        { }

        public override int GetMessageId()
        {
            return ID;
        }

        private StopMessage()
        {}
    }

}