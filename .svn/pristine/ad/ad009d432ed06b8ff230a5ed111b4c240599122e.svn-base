using ProtoBuf;
using VRage.Utils;
using Vsi.Network;

namespace Vsi.AutoMiner
{

    [ProtoContract]
    public class StartMessage : EntityMessage<StartMessage>
    {
        public static readonly int ID = (int)MyStringHash.GetOrCompute("VSI.AutominerStart");

        [ProtoMember(1)]
        public override long EntityId { get; protected set; }

        public StartMessage(long entityId)
            : base(entityId)
        { }

        public override int GetMessageId()
        {
            return ID;
        }

        private StartMessage()
        {}
    }

}